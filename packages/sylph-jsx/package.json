{
  "name": "sylph-jsx",
  "version": "0.0.0",
  "description": "A lightweight, SolidJS-powered runtime for building declarative PixiJS experiences",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./jsx-runtime.d.ts",
      "default": "./dist/index.js"
    },
    "./jsx-dev-runtime": {
      "types": "./jsx-runtime.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "jsx-runtime": [
        "./jsx-runtime.d.ts"
      ],
      "jsx-dev-runtime": [
        "./jsx-runtime.d.ts"
      ]
    }
  },
  "scripts": {
    "test": "vitest",
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}' --max-warnings 0"
  },
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "jsx-runtime.d.ts"
  ],
  "keywords": [
    "typescript",
    "jsx",
    "pixi.js",
    "solidjs",
    "pixi",
    "solid",
    "reactive",
    "signal",
    "gamedev"
  ],
  "author": "me@davecampion.com",
  "license": "MIT",
  "peerDependencies": {
    "pixi.js": "8.14.0",
    "solid-js": "1.9.4"
  },
  "devDependencies": {
    "tsc-alias": "^1.8.16",
    "@pixi/devtools": "^2.0.1",
    "vite-plugin-solid": "2.11.9"
  }
}
